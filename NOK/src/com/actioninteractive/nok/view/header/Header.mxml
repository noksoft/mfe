<?xml version="1.0" encoding="utf-8"?>
<header:HeaderBase 
	xmlns:fx="http://ns.adobe.com/mxml/2009" 
	xmlns:s="library://ns.adobe.com/flex/spark" 
	xmlns:mx="library://ns.adobe.com/flex/mx" 
	xmlns:header="com.actioninteractive.nok.view.header.*"
	>
	
	<fx:Declarations>
		<!-- Place non-visual elements (e.g., services, value objects) here -->
	</fx:Declarations>
	
	<header:PortletMenu id="portletMenu"
		x="171" y="50"
		portletList="{commonModel.currentSection.portletList}"
		/>
	
	<s:Image id="orangeBar"
		x="616" y="49"
		width="{this.width - orangeBar.x}"
		source="@Embed(source='assets/maqzar_assets.swf', symbol='hd_orangeBar')"
		mouseEnabled="false" mouseChildren="false"
		/>
	
	<header:SectionContainer 
		left="188"
		sectionArray="{commonModel.sectionList}"
		/>
	
	<header:UserMenu id="userMenu"
		width="174"
		top="12" right="12"
		/>
	
</header:HeaderBase>
