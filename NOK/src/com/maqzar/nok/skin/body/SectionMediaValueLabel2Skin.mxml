<GraphicBorderSkin xmlns:mx="http://www.adobe.com/2006/mxml"
				   xmlns="http://www.degrafa.com/2007">
	
	<geometry>
		<RoundedRectangle x="0" y="0" width="{skinWidth}" height="{skinHeight}" cornerRadius="10">
			<fill>
				<SolidFill color="0xFFFFFF" />
			</fill>
		</RoundedRectangle>
		<RoundedRectangle x="0" y="0" width="{skinWidth}" height="{skinHeight}" cornerRadius="10">
			<fill>
				<LinearGradientFill angle="90">
					<GradientStop color="#d6d6d6" />
					<GradientStop color="#ffffff" />
				</LinearGradientFill>
			</fill>
		</RoundedRectangle>
	</geometry>
</GraphicBorderSkin>
