<GraphicBorderSkin xmlns:mx="http://www.adobe.com/2006/mxml"
				   xmlns="http://www.degrafa.com/2007">
	
	<geometry>
		<RegularRectangle id="RegularRectangle1" height="{skinHeight}" width="{skinWidth}">
				<fill>
					<LinearGradientFill angle="90">
						<GradientStop alpha="0.8" color="#FFFFFF" ratio="0"/>
						<GradientStop alpha="0" color="#FFFFFF" ratio="0.8"/>
					</LinearGradientFill>
				</fill>
		</RegularRectangle>
	</geometry>
</GraphicBorderSkin>